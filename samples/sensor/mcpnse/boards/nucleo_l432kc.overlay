&i2c1 {
	clock-frequency = <I2C_BITRATE_FAST>;
    status = "okay";
    pinctrl-0 = <&i2c1_scl_pb6 &i2c1_sda_pb7>;

	mcpnse@18 {
		compatible = "microchip,mcpnse";
		reg = <0x18>;
		resolution = <3>;
		label = "MCPNSE";
		led-gpios = <&gpioa 11 GPIO_ACTIVE_HIGH>, <&gpiob 5 GPIO_ACTIVE_HIGH>, <&gpiob 4 GPIO_ACTIVE_HIGH>;
	};
};
